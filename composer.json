{
    "name": "phraseanet/composer-installer",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "A Phraseanet framework Composer installer",
    "keywords": [
        "installer",
        "Phraseanet"
    ],
    "autoload": {
        "psr-4": { "Composer\\Installers\\": "src/" }
    },
    "extra": {
        "class": "Composer\\Installers\\PhraseanetPlugin",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "composer-plugin-api": "1.0.0"
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "phpunit/phpunit": "~4.5"
    }
}
